{
  "name": "webissues",
  "version": "2.0.1",
  "description": "WebIssues team collaboration system",
  "productName": "WebIssues",
  "scripts": {
    "build": "npm run build:web && npm run build:electron && npm run build:main",
    "build:web": "webpack --config src/build/webpack.config.js --env.production",
    "build:electron": "webpack --config src/build/webpack.config.js --env.electron --env.production",
    "build:main": "webpack --config app/src/build/webpack.config.js --env.production",
    "dev:web": "webpack-dev-server --config src/build/webpack.config.js",
    "dev:electron": "webpack-dev-server --config src/build/webpack.config.js --env.electron",
    "start": "electron ./app/assets/js/main.min.js",
    "start:dev": "electron ./app/src/main.js",
    "package": "node ./app/src/build/package.js",
    "package:win32": "node ./app/src/build/package.js win32 ia32",
    "package:darwin": "node ./app/src/build/package.js darwin x64",
    "package:linux-ia32": "node ./app/src/build/package.js linux ia32",
    "package:linux-x64": "node ./app/src/build/package.js linux x64",
    "package:server": "node ./src/build/package.js",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register src/test.js src/**/*.spec.js",
    "test:watch": "cross-env NODE_ENV=test mocha --require @babel/register --watch src/test.js src/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mimecorg/webissues.git"
  },
  "author": "WebIssues Team",
  "license": "AGPL-3.0-or-later",
  "homepage": "https://webissues.mimec.org",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.4.1",
    "compare-versions": "^3.6.0",
    "core-js": "^3.6.5",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.18.3",
    "path-to-regexp": "^6.1.0",
    "vue": "^2.6.12",
    "vue-i18n": "^8.21.0",
    "vuedraggable": "^2.24.1",
    "vuex": "^3.5.1",
    "whatwg-fetch": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "archiver": "^5.0.0",
    "assets-webpack-plugin": "^5.1.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "chai-datetime": "^1.7.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "electron": "^9.3.0",
    "electron-packager": "^15.0.0",
    "file-loader": "^6.0.0",
    "glob": "^7.1.6",
    "less": "^3.12.2",
    "less-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.10.0",
    "mkdirp": "^1.0.4",
    "mocha": "^8.1.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "optionalDependencies": {
    "electron-installer-debian": "^2.0.1",
    "electron-installer-dmg": "^3.0.0",
    "electron-installer-redhat": "^2.0.0"
  }
}
